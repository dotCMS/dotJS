{
  "name": "dotcms-js",
  "version": "0.0.1",
  "description": "Set of components and services which allow you to connect to use data from a dotCMS installation",
  "scripts": {
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "start": "electron main.js",
    "build": "webpack --progress --profile --colors --display-error-details --display-cached",
    "watch": "webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "show": "webpack-dev-server --config showcase/config/webpack.dev.js --inline --progress --port 7070",
    "show-build": "rimraf dist && webpack --config showcase/config/webpack.prod.js --progress --profile --bail",
    "show-deploy": "git-directory-deploy --directory showcase/dist --branch gh-pages"
  },
  "author": "dotCMS",
  "license": "GPL 3",
  "devDependencies": {
    "@types/selenium-webdriver": "^2.53.1",
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^2.2.4",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "git-directory-deploy": "^1.5.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "json-loader": "^0.5.4",
    "null-loader": "^0.1.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "typescript": "^2.0.2",
    "typings": "^0.8.1",
    "uglify-js": "^2.6.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "angular2-logger": "^0.5.0",
    "es6-shim": "^0.35.1",
    "primeng": "^1.0.0-beta.17",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.12",
    "uglify-js": "^2.6.1",
    "zone.js": "^0.6.12",
    "core-js": "^2.4.1"
  },
  "gh-pages-deploy": {
    "staticpath": "showcase/dist",
    "noprompt": false
  }
}
